generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Request {
  id        String   @id @default(uuid())
  email     String
  status    String   @default("new") // new, fetching, gen_pdf, sending, done
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}